//abs mutlak değerini alır
trace(Math.abs(12.3));//12.3
//ceil bir üst tam sayıya yuvarlar
trace(Math.ceil(15.001));//16
//E sabitini verir.
trace(Math.E);//2.718281828459045
//Floor ondalık sayının tam kısmını kullanır.
trace(Math.floor(25.15));//25
//Verilen iki rakam arasındaki büyük olan rakamı seçer
trace(Math.max(15,14));//15
//Verilen iki rakam arasındaki küçük olan rakamı seçer
trace(Math.min(15,14));//14
//Pi sabit sayısını verir
trace(Math.PI);
//Sayının üssünü hesaplar
trace(Math.pow(3,2));//9
//En yakın tam sayıya yuvarlar
trace(Math.round(25.99));//26
trace(Math.round(25.49));//25
//Verilen rakamın karekökünü alır.
trace(Math.sqrt(4));//2
//0.5in karekökünü alan sabittir
trace(Math.SQRT1_2);
//2nin karekökünü alan sabittir
trace(Math.SQRT2);
//0 la 1 arasında rastgele sayı verir
trace(Math.random());
